    "def satisfiesF(L):\n",
    "    L_new = [s for s in L if f(s) == False]\n",
    "    L[:] = L_new\n",
    "    return len(L_new)\n",
    "\n",
    "def f(s):\n",
    "    return 'a' in s\n",
    "def run_satisfies(L, satisfiesF):\n",
    "    print(satisfiesF(L))\n",
    "    \n",
    "L = ['a','b','c']\n",
    "print(satisfiesF(L))\n",
    "print(L)"
   ]
